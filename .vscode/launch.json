{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug Streamlit app",
            "type": "debugpy",
            "request": "launch",
            "program": "-m",
            "args": [
                "streamlit",
                "run",
                "app.py"
            ],
            "cwd": "${workspaceFolder}/full_model"
        }
    ]
}
